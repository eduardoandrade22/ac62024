# -*- coding: utf-8 -*-

'''
Escreva a sua solução aqui
Code your solution here
Escriba su solución aquí
'''
import math
ponto1=input().split(" ")
ponto2=input().split(" ")

x1=float(ponto1[0])
y1=float(ponto1[1])
x2=float(ponto2[0])
y2=float(ponto2[1])

resultado = math.sqrt((x2-x1)**2 + (y2-y1)**2)

print(f"%.4f"% resultado)




